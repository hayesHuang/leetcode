package leetcode;

public class PalindromeNumber {
	public boolean isPalindrome(int x) {
	      int reg=x;
	      int reverse = 0;
	      while(reg>0){
	    	  reverse=reverse*10+reg%10;
	    	  reg=reg/10;
	      }
	      if(reverse==x){
	    	  return true;
	      }else{
	    	  return false;
	      }
	}

